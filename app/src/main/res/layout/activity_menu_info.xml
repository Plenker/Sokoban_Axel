<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Menu_Info">

    <Button
        android:id="@+id/btnMenu"
        android:layout_width="102dp"
        android:layout_height="61dp"
        android:backgroundTint="#673AB7"
        android:text="Menu"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.058"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.914" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="701dp"
        android:layout_height="275dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.092"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.127"
        android:text="This Game is a copy of Sokoban
        \n try to solve the level
        \n move all boxes on the white tagged destinations
\n\n Music:
\n Level 1: Algae Bloom - Stop Thinking
\n Level 2: Algae Bloom - Stop Thinking
\n Level 3: next interpret - song name
\n Level 4: next interpret - song name
\n Level 5: Metronome - The Manifested
\n Level 6: Sephira - Able's Journey (Psyonysus Remix)
\n Level 7: next interpret - song name
\n Level 8: next interpret - song name
\n Level 9: next interpret - song name
\n Level 10: next interpret - song name
"/>
</androidx.constraintlayout.widget.ConstraintLayout>